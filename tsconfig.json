{
    "compilerOptions": {
        "module": "es2015",
        "target": "es5",
        "moduleResolution": "node",
        "sourceMap": false,
        "declaration": true,
        "removeComments": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "listFiles": false,
        "noLib": false,
        "outDir": "dist",
        "noUnusedLocals": true,
        "lib": [
            "es2015",
            "dom"
        ],
        "strictNullChecks": true,
        "skipLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "@angular/*": [
                "../node_modules/@angular/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "bundles",
        "dist"
    ]
}